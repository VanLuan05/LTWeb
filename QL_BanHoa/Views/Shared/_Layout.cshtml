<!--Header-->
<div class="site-header">
    <nav class="navbar navbar-expand-lg navbar-dark">
        <div class="container-fluid">

            <!-- Logo -->
            <a href="#" class="navbar-brand">
                <b>
                    <span style="color: #036b03">BLOOM</span>
                    <span style="color: #d13364">Flower</span>
                </b>
            </a>

            <!-- Nút toggle khi thu nhỏ màn hình -->
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarMenu" aria-controls="navbarMenu" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <!-- Menu (có dropdown) -->
            <div class="collapse navbar-collapse " id="navbarMenu">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0 ">
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("Index", "Home")">Trang chủ</a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("Index", "SanPham")">Sản phẩm</a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("Index", "GioiThieu")">Giới thiệu</a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("Index", "LienHe")">Liên hệ</a>
                    </li>
                </ul>

                <!-- Form tìm kiếm (bên phải) -->
                <form class="d-flex but" action="@Url.Action("TimKiemSP", "Home")" method="get">
                    <input class="form-control me-2 custom-search" type="search" name="keyword" placeholder="Tìm hoa xinh..." aria-label="Search">
                    <button class="btn btn-outline-success text-white" type="submit">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-search" viewBox="0 0 16 16">
                            <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001q.044.06.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1 1 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0" />
                        </svg>
                    </button>
                </form>
                <div id="giohangmh">
                    <a href="@Url.Action("Index", "GioHang")" class="position-relative" style="text-decoration:none; color: #333; margin-right: 15px;">

                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24">
                            <path d="M6 9V7.25C6 3.845 8.503 1 12 1s6 2.845 6 6.25V9h.5a2.5 2.5 0 0 1 2.5 2.5v8a2.5 2.5 0 0 1-2.5 2.5h-13A2.5 2.5 0 0 1 3 19.5v-8A2.5 2.5 0 0 1 5.5 9Zm-1.5 2.5v8a1 1 0 0 0 1 1h13a1 1 0 0 0 1-1v-8a1 1 0 0 0-1-1h-13a1 1 0 0 0-1 1Zm3-4.25V9h9V7.25c0-2.67-1.922-4.75-4.5-4.75-2.578 0-4.5 2.08-4.5 4.75Z"></path>
                        </svg>

                        @if (Session["SoLuong"] != null && (int)Session["SoLuong"] > 0)
                        {
                            <span id="cart-count" class="position-absolute top-0 translate-middle badge rounded-pill bg-danger" style="left: 75%; font-size: 0.6rem; padding: 0.35em 0.5em;">
                                @Session["SoLuong"]
                                <span class="visually-hidden">unread messages</span>
                            </span>
                        }
                    </a>
                    <div class="user-dropdown dropdown">
                        @if (Session["TenKH"] != null)
                        {
                            <a href="#" class="d-flex align-items-center text-decoration-none dropdown-toggle" id="userMenuBtn" data-bs-toggle="dropdown" aria-expanded="false" style="color: #333;">

                                <div class="user-avatar-circle me-2">
                                    <i class="fas fa-user"></i>
                                </div>

                                <div class="d-none d-md-block">
                                    <span class="fw-bold" style="font-size: 0.9rem;">@Session["TenKH"]</span>
                                </div>
                            </a>

                            <ul class="dropdown-menu dropdown-menu-end shadow border-0 mt-2" aria-labelledby="userMenuBtn">
                                <li>
                                    <div class="px-3 py-2 border-bottom">
                                        <span class="small text-muted">Xin chào,</span><br>
                                        <strong style="color: #036b03;">@Session["TenKH"]</strong>
                                    </div>
                                </li>

                                <li>
                                    <a class="dropdown-item py-2" href="@Url.Action("Index", "DonHang")">
                                        <i class="fas fa-box-open me-2 text-success"></i> Đơn hàng của tôi
                                    </a>
                                </li>
                                <a class="dropdown-item py-2" href="@Url.Action("Index", "Profile")">
                                    <i class="fas fa-id-card me-2 text-primary"></i> Thông tin tài khoản
                                </a>

                                <li><hr class="dropdown-divider"></li>

                                <li>
                                    <a class="dropdown-item py-2 text-danger fw-bold" href="@Url.Action("LogOut", "Login")">
                                        <i class="fas fa-sign-out-alt me-2"></i> Đăng xuất
                                    </a>
                                </li>
                            </ul>
                        }
                        else
                        {
                            <a href="#" class="d-flex align-items-center text-decoration-none" data-bs-toggle="dropdown" aria-expanded="false" style="color: #333;">
                                <div class="user-avatar-circle bg-light text-secondary me-2">
                                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 16 16" width="18" height="18" fill="currentColor">
                                        <path d="M8 16A8 8 0 1 1 8 0a8 8 0 0 1 0 16Zm.847-8.145a2.502 2.502 0 1 0-1.694 0C5.471 8.261 4 9.775 4 11c0 .395.145.995 1 .995h6c.855 0 1-.6 1-.995 0-1.224-1.47-2.74-3.153-3.145Z"></path>
                                    </svg>
                                </div>
                              
                            </a>

                            <ul class="dropdown-menu dropdown-menu-end shadow border-0 mt-2">
                                <li><a class="dropdown-item" href="@Url.Action("Index", "Login")"><i class="fas fa-sign-in-alt me-2"></i> Đăng nhập</a></li>
                                <li><a class="dropdown-item" href="@Url.Action("Index", "DangKy")"><i class="fas fa-user-plus me-2"></i> Đăng ký</a></li>
                            </ul>
                        }
                    </div>
                </div>
            </div>
        </div>
    </nav>
</div>
